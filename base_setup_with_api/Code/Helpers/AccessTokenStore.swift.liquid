import Foundation

struct AccessToken {
    let value: String
}

protocol AccessTokenStore {
    var token: AccessToken? { get set }
}

class SessionTokenStore: AccessTokenStore {
    
    var token: AccessToken?
    
    static let shared = SessionTokenStore()
    
    private init() {}
}